<?xml version="1.0" encoding="UTF-8"?>

<configs>
	<!-- 服务ID -->
	<id>smServer</id>
	<!-- 
	         调试阶段，未接短信猫，以备业务系统感觉到发短信成功:设置为true 
	        正式运行阶段，设置为false ;
	-->
	<debugForNoModel>true</debugForNoModel>
	<!-- 短信猫所在的串口 -->
	<device>COM6</device>
	<!-- 串口波特率 -->
	<baudrate>9600</baudrate>
	
	<!-- 短信猫类型，目前只支持两类 人大金仓(jincang)和金笛(jindi) -->
	<modelType>jincang</modelType>

	<!-- 短信猫ID (金笛专用)-->
	<smId>automic.sm.model</smId>
	<!-- 短信猫芯片厂家(法国Wavecom或中国Huawei) (金笛专用)-->
	<factory>Wavecom</factory>
	
	<!-- 短信猫序列号 (金仓专用)-->
	<sn>RERE-IIRG-BRHB-IGBK</sn>
	
	<!-- 
	只能通过短信通道发送的命令功能码,
	GPRS通道不能发送该命令，例如短信唤醒功能
	 -->
	<codeOnlyBySm>ffff</codeOnlyBySm>
	<!-- 
	可以通过短信通道发送的命令功能码 
	-->
	<codeEnableBySm>0080_w</codeEnableBySm>
	
</configs>