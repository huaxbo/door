<?xml version="1.0" encoding="UTF-8"?>
<!-- 
  各配置元素名称不能重名，否则将覆盖;
  各配置元素名称已经与程序关联，不能修改！
 -->
<configs>
	<!-- 处理测控器数据线程池最大工作线程数, 取值范围1-100-->
	<remote_maxThreadNum>100</remote_maxThreadNum>
	<!-- 处理测控器数据线程池最小工作线程数, 取值范围1-100 -->
	<remote_minThreadNum>5</remote_minThreadNum>
	<!-- 
	     处理测控器数据线程池的工作线程，当其空闲时长为配置值时，
	      线程池将回收线程，以使线程池保持最小线程数  
	      单位为毫秒(1秒=1000毫秒)
	      限制不能小于1000
	 -->
	<remote_freeTimeout>60000</remote_freeTimeout>
	<!-- 
	      处理测控器数据线程池的工作线程，当其不间断工作时长为配置值时，
	      系统将认为线程已经了崩溃，将强制清除  
	      单位为毫秒(1秒=1000毫秒)
	      限制不能小于1000
	 -->
	<remote_busyTimeout>60000</remote_busyTimeout>
	
	<!-- 处理测缓存命令线程池最大工作线程数, 取值范围1-100-->
	<cachCom_maxThreadNum>100</cachCom_maxThreadNum>
	<!-- 处理测缓存命令线程池最小工作线程数, 取值范围1-100 -->
	<cachCom_minThreadNum>5</cachCom_minThreadNum>
	<!-- 
	     处理测缓存命令线程池的工作线程，当其空闲时长为配置值时，
	      线程池将回收线程，以使线程池保持最小线程数  
	      单位为毫秒(1秒=1000毫秒)
	      限制不能小于1000
	 -->
	<cachCom_freeTimeout>60000</cachCom_freeTimeout>
	<!-- 
	      处理测缓存命令线程池的工作线程，当其不间断工作时长为配置值时，
	      系统将认为线程已经了崩溃，将强制清除  
	      单位为毫秒(1秒=1000毫秒)
	      限制不能小于1000
	 -->
	<cachCom_busyTimeout>60000</cachCom_busyTimeout>
</configs>