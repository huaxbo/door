<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!-- 监控服务的ip地址,发送系统运行状态(UDP数据包)至监控系统时使用 -->
	<ip>192.168.2.250</ip>
	<!-- 监控服务端口号-->
	<port>6666</port>
	<!-- 行政区划码(6位)+系统标识(业务平台：1; 发布服务：2; 数据同步：3; 通讯服务：4) 如烟台招远县 山洪系统  3706851-->
	<systemId>3706851</systemId>
	<!-- 发送系统运行状态自动任务重复执行间隔(秒)，只能取整数 -->
	<repeatInterval>3</repeatInterval>
	<!-- 数据库驱动类(用户检测系统连接数据是否正常) -->
	<database.driver>net.sourceforge.jtds.jdbc.Driver</database.driver>
	<!-- 数据库url -->
	<database.url>jdbc:jtds:sqlserver://192.168.0.4:1433;DatabaseName=flood_v4_0;useUnicode=true;characterEncoding=GBK;autoReconnect=true</database.url>
	<!-- 数据库用户名 -->
	<database.username>sa</database.username>
	<!-- 数据库密码 -->
	<database.password>swdeptsa654321</database.password>
</config>